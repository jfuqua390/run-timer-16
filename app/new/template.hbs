{{outlet}}
<div class="new-run">
    <form class="new-run__form">
      <h2 class="new-run__title">New Run</h2>
      <input type="text" class="new-run__input run-time" placeholder="Run Time">
      <input type="text" class="new-run__input run-date" placeholder="Race Date">
      <input type="text" class="new-run__input run-notes" placeholder="Race Notes">
      <div class="button-row">
        <button class="cancel-button">Cancel</button>
        <button class="save-button">Save</button>
      </div>
    </form>
</div>
