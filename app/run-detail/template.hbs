{{outlet}}
<div class="run-detail">
  <div class="run-detail__title">
    <h2 class="run-detail__title-text">My Run</h2>
  </div>
  <div class="run-detail__info">
    <p class="info__date">
      {{model.date}}
    </p>
    <p class="info__time">
      {{model.time}}
    </p>
  </div>
  <div class="run-detail__notes">
    <p class="notes__text">
      {{model.notes}}
    </p>
  </div>
  <div class="run-detail__buttons">
    <span class="fa fa-angle-left"></span>
    <span class="fa fa-pencil"></span>
  </div>
</div>
